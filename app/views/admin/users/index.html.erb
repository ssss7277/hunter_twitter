<h1>ユーザー一覧</h1>
<%= link_to '投稿一覧', admin_posts_path %>
<p id="alert" class="sherif"><%= flash[:notice]%></p>
<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td>
          <%= link_to user.name, edit_admin_user_path(user.id) %>
        </td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
